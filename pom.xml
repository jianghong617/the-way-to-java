<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.5.6</version>
    </parent>
    <groupId>com.photowey</groupId>
    <artifactId>the-way-to-java</artifactId>
    <version>1.0.0-SNAPSHOT</version>
    <packaging>pom</packaging>

    <name>the-way-to-java</name>
    <description>the-way-to-java parent project</description>

    <properties>
        <java.version>11</java.version>
        <fastjson.version>1.2.75</fastjson.version>
    </properties>

    <modules>
        <module>dubbo-in-action</module>
        <module>kafka-in-action</module>
        <module>mybatis-in-action</module>
        <module>netty-in-action</module>
        <module>rabbitmq-in-action</module>
        <module>redis-in-action</module>
        <module>rocketmq-in-action</module>
        <module>zookeeper-in-action</module>
    </modules>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>com.alibaba</groupId>
                <artifactId>fastjson</artifactId>
                <version>${fastjson.version}</version>
            </dependency>
        </dependencies>
    </dependencyManagement>

</project>
